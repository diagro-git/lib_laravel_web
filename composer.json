{
    "name": "diagro/lib_laravel_web",
    "description": "Library for webapplications written in Laravel",
    "type": "library",
    "version": "2.0",
    "authors": [
        {
            "name": "Stijn Leenknegt",
            "email": "stijn@diagro.be"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/diagro-git/lib_laravel_token.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/diagro-git/lib_laravel_api.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework":"^9.0",
        "diagro/lib_laravel_token": "^1.0",
        "diagro/lib_laravel_api": "^2.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Diagro\\Web\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diagro\\Web\\DiagroServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {

        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
